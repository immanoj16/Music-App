<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mymusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TracksActivity"
            android:label="@string/tracks" />
        <activity
            android:name=".AlbumsActivity"
            android:label="@string/albums" />
        <activity
            android:name=".ArtistsActivity"
            android:label="@string/artists" />
        <activity
            android:name=".PlayingActivity"
            android:label="@string/playing" />
        <activity
            android:name=".PlaylistsActivity"
            android:label="@string/playlists" />
        <activity
            android:name=".PaymentActivity"
            android:label="@string/payment"/>
    </application>

</manifest>